# Phase 2: DNS Configuration (Production)
ENVIRONMENT=production
PORTAL_IP=elevatediq.ai
PORTAL_PORT=443
BASE_PATH=/portal

# GCP Configuration
GCP_PROJECT_ID=portal-prod
GCP_REGION=us-central1

# Logging
LOG_LEVEL=INFO

# Cache (Redis)
REDIS_URL=redis://cache.internal:6379/0
CACHE_TTL=3600

# Timeouts
REQUEST_TIMEOUT=30

# SSL/TLS
ENABLE_HTTPS=true
SSL_CERT_PATH=/etc/certs/tls.crt
SSL_KEY_PATH=/etc/certs/tls.key
