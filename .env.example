# GCP Landing Zone Portal - Environment Configuration
# Copy this to .env and customize for your deployment

# Environment Type (development|staging|production)
ENVIRONMENT=development

# Network Configuration - Use IP address, NOT localhost
LOCAL_IP=192.168.168.42
PORTAL_PORT=5173
API_PORT=8080

# DNS Configuration for Different Environments
# DEV Environment
DEV_DOMAIN=dev.elevatedoq.ai
DEV_API_URL=https://dev.elevatedoq.ai/lz
DEV_PORTAL_URL=https://dev.elevatedoq.ai/portal

# QA/STAGING Environment
QA_DOMAIN=qa.elevatedoq.ai
QA_API_URL=https://qa.elevatedoq.ai/lz
QA_PORTAL_URL=https://qa.elevatedoq.ai/portal

# PROD Environment
PROD_DOMAIN=elevatedoq.ai
PROD_API_URL=https://elevatedoq.ai/lz
PROD_PORTAL_URL=https://elevatedoq.ai/portal

# GCP Configuration
GCP_PROJECT_ID=landing-zone-dev
GCP_REGION=us-central1

# Authentication
REQUIRE_AUTH=false
ALLOW_DEV_BYPASS=true

# Redis Configuration
REDIS_URL=redis://redis:6379/0
REDIS_MAX_CONNECTIONS=50

# Logging
LOG_LEVEL=DEBUG

# CORS - Default to local IP address, add environment DNS URLs
# Format: http://192.168.168.42:5173,https://dev.elevatedoq.ai,https://qa.elevatedoq.ai,https://elevatedoq.ai

# API Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=1000

# Database
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=landing_zone
DATABASE_USER=postgres

# Timeouts
API_TIMEOUT_SECONDS=30
HEALTH_CHECK_INTERVAL_SECONDS=30

# Feature Flags
ENABLE_RCA_ANALYSIS=true
ENABLE_NOTIFICATIONS=true
ENABLE_CACHING=true
