# GCP Landing Zone Portal - Environment Configuration
# Copy this to .env and customize for your deployment

# Environment Type
ENVIRONMENT=production

# Network Configuration
HOST_IP=192.168.168.42
PORTAL_PORT=5173
API_PORT=8080

# GCP Configuration
GCP_PROJECT_ID=landing-zone-prod
GCP_REGION=us-central1

# Authentication
REQUIRE_AUTH=false
ALLOW_DEV_BYPASS=false

# Redis Configuration
REDIS_URL=redis://redis:6379/0
REDIS_MAX_CONNECTIONS=50

# Logging
LOG_LEVEL=INFO

# CORS
ALLOWED_ORIGINS=http://192.168.168.42:5173,https://elevatediq.ai,https://www.elevatediq.ai

# API Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=1000

# Database
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=landing_zone
DATABASE_USER=postgres

# Timeouts
API_TIMEOUT_SECONDS=30
HEALTH_CHECK_INTERVAL_SECONDS=30

# Feature Flags
ENABLE_RCA_ANALYSIS=true
ENABLE_NOTIFICATIONS=true
ENABLE_CACHING=true

# GitHub Integration (for task delegation)
GITHUB_TOKEN=ghp_your_github_personal_access_token_here

# Cloud Tasks Configuration (for task delegation)
CLOUD_TASKS_LOCATION=us-central1
CLOUD_TASKS_QUEUE=github-issue-delegation
TASK_HANDLER_URL=https://your-cloud-run-service.run.app/handle-task
